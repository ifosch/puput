#!/usr/bin/env python

import click

import picot.feed
from puput import date_filter


@click.command()
@click.argument('url', nargs=1)
def collect(url):
    """This collects all messages for url."""
    entries = picot.feed.Feed(
        url,
    )
    for entry in entries:
        print(entry['title'])


if __name__ == '__main__':
    collect()
